<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.web.mapperImpl.UserPhoneMapperImpl">
	<insert id="insertPhone" parameterType="cmd">
		INSERT INTO UserPhone VALUES (#{userPhone.phoneid}, #{userPhone.id}, #{userPhone.seq}, #{userPhone.phoneNumber}, #{userPhone.telecom})
	</insert>
	<select id="selectPhone" resultType="phone" parameterType="cmd">
		SELECT * FROM UserPhone WHERE phoneid like #{userPhone.phoneid}
	</select>
</mapper>